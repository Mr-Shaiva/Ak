public class Solution {
    public int[][] solve(int[][] A) {
        int m = A.length;
        int n = A[0].length;

        int[][] temp = new int[n][m];

        for(int i=0;i<m;i++){
            for(int j=0;j<n;j++){
                temp[j][i] = A[i][j];
            }
        }

        return temp;
    }
}
